<h2 id="title">Welcome to Ember</h2>

{{#with-service 'session' as |session|}}
  {{#if session.isAuthenticated}}
    {{#with model.currentUser as |user|}}
      <h3>
        Current User
      </h3>

      <p>
        id: {{user.id}}
      </p>

      <p>
        displayName: {{user.displayName}}
      </p>

      <p>
        <a
          href
          {{action (route-action 'logout')}}
        >Logout</a>
      </p>
    {{/with}}
  {{/if}}
{{/with-service}}

<hr>

{{outlet}}
